	<PropertyGroup>
		<CopyLocalLockFileAssemblies>false</CopyLocalLockFileAssemblies>
		<ResolveAssemblyWarnOrErrorOnTargetArchitectureMismatch>
			None
		</ResolveAssemblyWarnOrErrorOnTargetArchitectureMismatch>
		<LangVersion>13.0</LangVersion>
		<!-- <PreBuildEvent> -->
			<!-- if not exist "$(MSBuildProjectDirectory)\bin\$(Configuration)\FamilyDeveloper.addin" copy "$(MSBuildProjectDirectory)\FamilyDeveloper.addin" "$(MSBuildProjectDirectory)\bin\$(Configuration)\" -->
		<!-- </PreBuildEvent> -->
	</PropertyGroup>

	<PropertyGroup>
		<TargetFramework>net8.0-windows</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>warnings</Nullable>
		<UseWPF>true</UseWPF>
		<CopyLocalLockFileAssemblies>false</CopyLocalLockFileAssemblies>
		<RootNamespace>FamilyDeveloper</RootNamespace>
		<AssemblyName>FamilyDeveloper</AssemblyName>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<StartAction>Program</StartAction>
		<StartProgram>C:\Program Files\Autodesk\Revit 20$([System.Text.RegularExpressions.Regex]::Match($(Configuration), `\d\d`))\Revit.exe</StartProgram>
		<Version>$(VersionPrefix)0.2.0.0</Version>
	</PropertyGroup>

	<PropertyGroup Condition="$(Configuration.Contains('Debug'))">
		<NoWarn>1701;1702;MSB3277</NoWarn>
		<StartArguments>"$(MSBuildProjectDirectory)\bin\$(Configuration)\journal.0001easyexample.txt"</StartArguments>
	</PropertyGroup>

	<PropertyGroup Condition="$(Configuration.Contains('Release'))">
		<NoWarn>1701;1702;MSB3277</NoWarn>
	</PropertyGroup>

