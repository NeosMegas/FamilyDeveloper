	<Target Name="PreBuild" BeforeTargets="PreBuildEvent">
		<Exec Condition="$(Configuration.Contains('Debug'))" Command="if not exist &quot;$(MSBuildProjectDirectory)\bin\$(Configuration)\journal.0001easyexample.txt&quot; copy &quot;$(MSBuildProjectDirectory)\journal.0001easyexample.txt&quot; &quot;$(MSBuildProjectDirectory)\bin\$(Configuration)\&quot;" />
		<Exec Command="if not exist &quot;$(MSBuildProjectDirectory)\bin\$(Configuration)\FamilyDeveloper.addin&quot; copy &quot;$(MSBuildProjectDirectory)\FamilyDeveloper.addin&quot; &quot;$(MSBuildProjectDirectory)\bin\$(Configuration)\&quot;" />
	</Target>
	
